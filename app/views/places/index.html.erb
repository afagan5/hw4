<div class="row">
      <div class="col-sm-9">
        <h1>Places</h1>
      </div>
      <div class="col-sm-3 text-end">
        <a href="/places/new/" class="btn btn-primary">New Place</a>
      </div>
    </div>

<div class="my-3">
  <div class="list-group">
  <% for place in @places %>
    <a href="/places/<%= place["id"]%>" class="list-group-item list-group-item-action"> <%= place["name"] %></a>
  <% end %>
  </div>
</div>



